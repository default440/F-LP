(defun all (p x)
    (dolist (element x) (cond ((not (funcall p element)) (setq result nil)) ((funcall p element) (setq result t))))
    result
)

(defun isatom (x)
    (cond ((atom x) (setq result t)) ((not (atom x))(setq result nil)))
    result
)

(setq lst1 '(0 (1 2) 2 (3)))
(setq lst2 '(1 1 1))

(write (all 'isatom lst1))
(terpri)
(write (all 'isatom lst2))
